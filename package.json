  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint": "^5.16.0",
    "franc": "^4.0.0",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "minimist": "^1.2.0",
    "request": "^2.88.0",
    "scrape-twitter": "^0.11.1",
    "sentiment": "^5.0.1",
